{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "ES6",
    "lib": [
      "es6",
      "esnext.asyncIterable"
    ]
  },
  "include": [
    "graphql/**/*",
    "generated/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
